pipeline {
  agent any
  stages {
    stage('Test') {
      steps {
        echo 'Start Test'
        sleep 29
        echo 'End Test'
      }
    }

    stage('Build') {
      steps {
        echo 'Start build'
        sleep 30
        echo 'ENd build'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Start Deploy'
        sleep 30
        echo 'End Deploy'
      }
    }

  }
}